AnyFolder ModelViews = {
    AnyFolder SetViewMacros = {
       AnyOperationMacro All_without_muscles = {
         MacroStr = {"classoperation Main.ModelSetup.ModelViews.All_NoMuscles "+strquote("Set View") };
       };    
       AnyOperationMacro All_without_marker_arrows = {
         MacroStr = {"classoperation Main.ModelSetup.ModelViews.All_NoMarkerCoords "+strquote("Set View") };
       };    
    };
    AnyFolder DrawGroups = {
      AnyDrawGroup All_Markers ={
        Objects = ObjSearchRecursive("Main.ModelSetup.MocapDrivers","*", "AnyDrawObject");
      };
      AnyDrawGroup All_MarkerArrows = {
        Objects = ObjSearchRecursive("Main.HumanModel.BodyModel","Arrows.*", "AnyDrawVector");
      };
      AnyDrawGroup All_Human ={
        Objects = ObjSearchRecursive("Main.HumanModel","*", "AnyDrawObject");
      };
      AnyDrawGroup All_Environment ={
        Objects = ObjSearchRecursive("Main.EnvironmentModel","*", "AnyDrawObject");
      };
      AnyDrawGroup All ={
        Objects = ObjSearchRecursive("Main","*", "AnyDrawObject");
      };
      AnyDrawGroup All_Muscles = 
      {
        Objects = arrcat( ObjSearchRecursive("Main.HumanModel.BodyModel","*", "AnyDrawMuscle"),
                          ObjSearchRecursive("Main.HumanModel.BodyModel.Trunk.Buckle", "*", "AnyDrawParamSurf"));
      };
      AnyDrawGroup CenterOfPressure= {
        Objects = arrcat( arrcat( ObjSearchRecursive( "Main.EnvironmentModel.ForcePlates", "CenterOfPressure.Line", "AnyDrawObject"),
                                  ObjSearchRecursive( "Main.EnvironmentModel.ForcePlates", "ForcePlate.COP_ball", "AnyDrawObject")),
                         ObjSearchRecursive( "Main.EnvironmentModel.ForcePlates", "ForcePlate.PlateSurface.COP_ball", "AnyDrawObject"));
      };
    };//DrawGroups
    AnyProjectModelViewDefinition ViewAll = {
      DrawGroupSequence = {&.DrawGroups.All};
      Reset = {Off};
      ShowModelDefined = {On};
    };
    AnyProjectModelViewDefinition KinematicView ={
      DrawGroupSequence = {&.DrawGroups.All, &.DrawGroups.All_Muscles, &.DrawGroups.CenterOfPressure};
      Reset = {On,Off, On};
      Hide = {Off, On, On}; //Same behavior as not defining Hide.
      ShowModelDefined = {On,Off, Off}; 
      ShowAutoGenerated = {Off,Off, Off}; //Same behavior as {Off, On}.
    };
    AnyProjectModelViewDefinition InverseDynamicView = {
      DrawGroupSequence = {&.DrawGroups.All, &.DrawGroups.All_MarkerArrows};
      Reset = {On,Off}; //Same behavior as not defining Reset.
      Hide = {Off, On}; //Same behavior as not defining Hide.
      ShowModelDefined = {On,Off}; 
      ShowAutoGenerated = {Off,Off}; //Same behavior as {Off, On}.
    };
//    AnyProjectModelViewDefinition Hide_Muscles = {
//      DrawGroupSequence = {&.DrawGroups.All_Muscles};
//      Reset = {On}; //Same behavior as not defining Reset.
//      Hide = {On}; //Same behavior as not defining Hide.
//      ShowModelDefined = {Off}; 
//      ShowAutoGenerated = {Off}; //Same behavior as {Off, On}.
//      Transparent = {Off}; 
//      Select = {Off}; //Same behavior as not defining Select.
//    };
    
        
    
};


